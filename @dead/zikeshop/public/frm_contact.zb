*# form for editing contacts (addresses)
*# $Id$
* exec:
    REQ = '<span style="color:red">*</span> '

<tr><td align="right">First Name:</td>
    <td><input type="text" name="fname" value="{:fname:}"> {:REQ:} </td></tr>
<tr><td align="right">Last Name:</td>
    <td><input type="text" name="lname" value="{:lname:}"> {:REQ:} </td><tr>
<tr><td align="right">Email Address:</td>
    <td><input type="text" name="email" value="{:email:}"> {:REQ:} </td><tr>

<tr><td align="right">Address 1:</td>
    <td><input type="text" size="40" name="address1"
               value="{:address1:}"> {:REQ:} </td></tr>
<tr><td align="right">Address 2:</td>
    <td> <input type="text" size="40" name="address2"
                value="{:address2:}"> {:REQ:} </td></tr>
<tr><td align="right">Address 3:</td>
    <td> <input type="text" size="40"name="address3"
                value="{:address3:}"> {:REQ:} </td></tr>
<tr><td align="right">City, State, Zip:</td>
    <td> <input type="text" name="city" value="{:city:}"> {:REQ:}&nbsp;
         <input type="text" name="stateCD"
                size="2" maxlength="2"  value="{:stateCD:}"> &nbsp;
         <input type="text" name="postal" size="5" value="{:postal:}"></td></tr>
